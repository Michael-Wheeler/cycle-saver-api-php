{
  "name": "michael-wheeler/cycle-saver-api-php",
  "description": "Calculate commute savings",
  "license": "MIT",
  "authors": [
    {
      "name": "Michael Wheeler",
      "email": "michael.wheeler856@gmail.com"
    }
  ],
  "require": {
    "php": "7.4",
    "ext-mongodb": "*",
    "ext-json": "*",
    "ext-http": "*",
    "slim/slim": "^4.0",
    "slim/psr7": "^1.0",
    "slim/http": "^1.0",
    "php-di/php-di": "^6.1",
    "psr/log": "^1.1",
    "vlucas/phpdotenv": "^4.1",
    "ramsey/uuid": "^4.0",
    "psr/http-message": "^1.0",
    "guzzlehttp/guzzle": "^6.5",
    "psr/http-client": "^1.0",
    "psr/http-factory": "^1.0",
    "psr/cache": "^1.0",
    "mongodb/mongodb": "^1.8"
  },
  "require-dev": {
    "phpunit/phpunit": "^9",
    "squizlabs/php_codesniffer": "*",
    "phpspec/prophecy-phpunit": "^2.0"
  },
  "config": {
    "platform": {
      "php": "7.4"
    }
  },
  "autoload": {
    "psr-4": {
      "CycleSaver\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "CycleSaver\\": [
        "tests/unit",
        "tests/integration",
        "tests/third_party",
        "tests/performance"
      ]
    }
  }
}
